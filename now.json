{
  "name": "platzistore-api",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "CORS": "*",
    "DB_USER": "@platzistore-db-user",
    "DB_PASSWORD": "@platzistore-db-password",
    "DB_HOST": "@platzistore-db-host",
    "DB_NAME": "@platzistore-db-name"
  }
}